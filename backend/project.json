{
  "name": "backend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "backend",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node server.js",
        "cwd": "backend"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node server.js",
        "cwd": "backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "backend"
      }
    }
  }
}
