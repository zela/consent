{
  "name": "consent-app",
  "version": "1.0.0",
  "description": "Consent data application with React frontend and Express backend",
  "private": true,
  "scripts": {
    "dev:backend": "nx run backend:dev",
    "dev:frontend": "nx run frontend:dev",
    "dev": "nx run-many --targets=dev --projects=backend,frontend --parallel",
    "build": "nx run frontend:build",
    "test": "nx run-many --targets=test --all",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "lint": "nx run-many --targets=lint --projects=backend,frontend --parallel",
    "lint:fix": "nx run-many --targets=lint --projects=backend,frontend --parallel -- --fix"
  },
  "devDependencies": {
    "eslint-config-prettier": "^10.1.5",
    "nx": "21.2.3",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
